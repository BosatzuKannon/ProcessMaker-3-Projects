        <dyn_content><![CDATA[{"name":"PUNTAJES","description":"","items":[{"type":"form","variable":"","var_uid":"","dataType":"","id":"6357422605d092f8b4c7613009645453","name":"PUNTAJES","description":"","mode":"edit","script":{"type":"js","code":"inicio();\n\n\nfunction inicio(){\n\n\t$(\"#textVar002\").hide();\n  \n  $(\"#subtitle0000000001\").css(\"color\", \"blue\");\n  $(\"#subtitle0000000002\").css(\"color\", \"red\");\n  $(\"#image0000000001\").css(\"background color\", \"#E69200\");  \n  //$(\"#textVar002\").getControl().maskNumber({decimal: ',', thousands: '.'});\n  //var a = dar_pausa();\n  //$(\"#textVar002\").getControl().maskNumber();\n  //alert(a);\n  startTime();\n  //formato();\n}\n\nfunction startTime() {\n  var today = new Date();\n  var h = today.getHours();\n  var m = today.getMinutes();\n  var s = today.getSeconds();\n  m = checkTime(m);\n  s = checkTime(s);\n  var mostrar = \"Hora Actual \"+h + \":\" + m + \":\" + s;\n  $(\"#subtitle0000000001\").setValue(mostrar);\n  var t = setTimeout(startTime, 500);\n}\n\nfunction checkTime(i) {\n  if (i < 10) {i = \"0\" + i};  // add zero in front of numbers < 10\n  return i;\n}\n\nfunction organizar_descendente(){\n\n  var numRowss = $(\"#gridVar001\").getValue();  \n  var numRows = numRowss.length;\n\n  var proponente = \"\";\n  var fc = \"\";\n  var ai = \"\";\n  var dis = \"\";\n  var v_i = \"\";\n\n  for (var i=1; i <= numRows; i++) {      \n\n      for (var j=2; j <= numRows; j++){\n\n        if(parseFloat($(\"#gridVar001\").getText(j, 5)) > parseFloat($(\"#gridVar001\").getText(j-1, 5))){\n\n          proponente = $(\"#gridVar001\").getText(j, 1);\n          fc = $(\"#gridVar001\").getText(j, 2);\n          ai = $(\"#gridVar001\").getText(j, 3);\n          dis = $(\"#gridVar001\").getText(j, 4);\n          v_i = parseFloat($(\"#gridVar001\").getText(j, 5));\n          \n          $(\"#gridVar001\").setText($(\"#gridVar001\").getText(j-1, 1),j, 1);\n          $(\"#gridVar001\").setText($(\"#gridVar001\").getText(j-1, 2),j, 2);\n          $(\"#gridVar001\").setText($(\"#gridVar001\").getText(j-1, 3),j, 3);\n          $(\"#gridVar001\").setText($(\"#gridVar001\").getText(j-1, 4),j, 4);\n          $(\"#gridVar001\").setText($(\"#gridVar001\").getText(j-1, 5),j, 5);\n\n          $(\"#gridVar001\").setText(proponente,j-1, 1);\n          $(\"#gridVar001\").setText(fc,j-1, 2);\n          $(\"#gridVar001\").setText(ai,j-1, 3);\n          $(\"#gridVar001\").setText(dis,j-1, 4);\n          $(\"#gridVar001\").setText(v_i,j-1, 5);\n\n          //break;\n        }\n      }        \n  }return 1;\n}\n\n$(\"#textVar001\").setOnchange(configureLocation);\n\n$(\"#textVar004\").setOnchange(calcular_aui);\n\nfunction calcular_aui(){\n\t\n  var pi = parseFloat($(\"#textVar003\").getText());\n  var cd = parseFloat($(\"#textVar004\").getText());\n  \n  if(pi != \"\" && cd != \"\"){\n  \t\n    var aui = pi - cd;\n    \n    var aui_por = (aui / cd) * 100;\n    \n    var total = \"%\" + aui_por + \" - $\" + aui.toFixed(2);\n    \n    $(\"#textVar005\").setText(total);\n  }\n}\n\nfunction configureLocation(){\n  \n  $(\"#textVar002\").hide();\n  var x = parseInt($(\"#textVar001\").getValue(), 10);\n\n  if ( x > 0 && x <= 19)\n  {        \n        $(\"#dropdownVar001\").setValue(1);\n  }\n  else if(x <= 39 && x > 19)\n  {\n        $(\"#dropdownVar001\").setValue(2);\n    \t$(\"#textVar002\").show();\n  }\n  else if(x <= 59 && x > 39)\n  {\n        $(\"#dropdownVar001\").setValue(3);     \n  }\n  else if(x <= 79 && x > 59)\n  {\n        $(\"#dropdownVar001\").setValue(4);     \n  }\n  else if(x <= 99 && x > 79)\n  {\n        $(\"#dropdownVar001\").setValue(5);     \n  }\t\n}\n\n\n$(\"#button0000000001\").find(\"button\").on(\"click\" , function() {\t  \n  \n  var a = desresaltar();\n  if($(\"#textVar001\").getValue() == \"\"){      \n      alert(\"Por favor ingrese los dos decimales de la TRM\");    \n  }\n\n  var numRowss1 = $(\"#gridVar001\").getValue();  \n  var numRows1 = numRowss1.length;\n  \n  if(numRows1 == 0){\n    alert(\"Para calcular los ponderados debe haber como mÃ­nimo 2 propuestas.\");\n  }\n  else{\n      var numRowss = $(\"#gridVar001\").getValue();  \n      var numRows = numRowss.length;\n      var count=0;\n\n      for (var i=1; i <= numRows; i++) {\n\n          if ($(\"#gridVar001\").getText(i, 5) == \"\") {\n              $(\"#gridVar001\").getControl(i, 5).css(\"border\",\"red solid 1px\");\n              count++;\n          }\n          else {\n              $(\"#gridVar001\").getControl(i, 5).css(\"border\",\"green solid 1px\");\n          }\n      }\n    \n      if (count > 0) {\n\n        alert(\"Por favor ingrese el campo de la Oferta Economica para todas las propuestas participantes.\");\n      }\n      else {\n        \n        var metodo = $(\"#dropdownVar001\").getValue();\n\n        if(metodo == \"2\" && $(\"#textVar002\").getValue() == \"\"){alert(\"Por favor ingrese el valor del Presupuesto Oficial del Proyecto\");}\n        else{\n\n          if(metodo == \"1\"){metodo1();}\n          else if(metodo == \"2\"){metodo2();}\n          else if(metodo == \"3\"){metodo3();}\n          else if(metodo == \"4\"){metodo4();}\n          else {metodo5();}\n        }          \n      }\n  }\n    \n});\n\nfunction metodo1(){\n\n  var r = organizar_descendente();\n  \n  var numRowss = $(\"#gridVar001\").getValue();  \n  var numRows = numRowss.length;\n  var mediana = 0;\n  var recibe = 0;\n\n  if (numRows % 2 == 0){\n\n    mediana = (parseFloat($(\"#gridVar001\").getText((Math.trunc(numRows/2)), 5)) + parseFloat($(\"#gridVar001\").getText(((Math.trunc(numRows/2))+1), 5)))/2;\n    recibe = Math.trunc(numRows/2)+1;\n  }\n  else{\n\n    mediana = parseFloat($(\"#gridVar001\").getText((Math.trunc(numRows/2)+1), 5));\n    recibe = Math.trunc(numRows/2)+1;\n  }\n\n  var puntaje_monetario=0;\n  var v_i =0;\n  var puntaje_total = 0;\n  var fc = false;\n  var ai = false;\n  var dis = false;\n\n  for (var i=1; i <= numRows; i++) {\n\n      v_i = parseFloat($(\"#gridVar001\").getText(i, 5));\n\n      if(i == recibe){\n        puntaje_monetario = 70;\n      }\n      else{\n        puntaje_monetario = 70 * (1 -  (Math.abs((mediana - v_i) / mediana)));\n      }\n            \n      $(\"#gridVar001\").setText(puntaje_monetario.toFixed(7),i, 6);\n\n      fc = $(\"#gridVar001\").getText(i, 2);\n      ai = $(\"#gridVar001\").getText(i, 3);\n      dis = $(\"#gridVar001\").getText(i, 4);\n\n      puntaje_total = puntaje_monetario;\n      \n      if(fc == \"true\"){\n        puntaje_total += 19;\n      }\n      if(ai == \"true\"){\n        puntaje_total += 10;\n      }\n      if(dis == \"true\"){\n        puntaje_total += 1;\n      }\n      \n      $(\"#gridVar001\").setText(puntaje_total.toFixed(7),i, 7);\n      $(\"#textVar003\").setText(propuesta_ideal_m1().toFixed(2));\n      $(\"#textVar003\").css(\"color\", \"green\");\n      \n  }\n  var ganador = resaltar_ganador();\n}\n\nfunction metodo2(){\t\n\n  var r = organizar_descendente();\n  var numRowss = $(\"#gridVar001\").getValue();  \n  var numRows = numRowss.length;\n  var m = obtener_cantidad_m();\n  var exponente = numRows + m;\n  var numero1 = Math.pow(parseFloat($(\"#textVar002\").getValue()),parseFloat(m));\n  var numero2 = obtener_multiplicacion_valores();\n  var mg_po=Math.pow((numero1 * numero2),1/exponente);\n\n  var puntaje_monetario=0;\n  var v_i =0;\n  var puntaje_total = 0;\n  var fc = false;\n  var ai = false;\n  var dis = false;\n\n  for (var i=1; i <= numRows; i++) {\n\n      v_i = parseFloat($(\"#gridVar001\").getText(i, 5));\n      \n      if(v_i <= mg_po){\n        puntaje_monetario = 70 * (1 - ((mg_po - v_i) / mg_po));\n      }\n      else{\n        puntaje_monetario = 70 * (1 - (2 * ( (Math.abs(mg_po - v_i)) / mg_po)));\n      }\n      \n      $(\"#gridVar001\").setText(puntaje_monetario.toFixed(7),i, 6);\n\n      fc = $(\"#gridVar001\").getText(i, 2);\n      ai = $(\"#gridVar001\").getText(i, 3);\n      dis = $(\"#gridVar001\").getText(i, 4);\n\n      puntaje_total = puntaje_monetario;\n      \n      if(fc == \"true\"){\n        puntaje_total += 19;\n      }\n      if(ai == \"true\"){\n        puntaje_total += 10;\n      }\n      if(dis == \"true\"){\n        puntaje_total += 1;\n      }\n      \n      $(\"#gridVar001\").setText(puntaje_total.toFixed(7),i, 7);\n      $(\"#textVar003\").setText(mg_po.toFixed(2));\n      $(\"#textVar003\").css(\"color\", \"green\");\n  }\n  var ganador = resaltar_ganador();\n}\n\nfunction metodo3(){\t\n\n  var r = organizar_descendente();\n  var v_max = calcular_propuesta_alta();\n  var x = obtener_promedio_general();\n  var x_a = (v_max + x) / 2;\n  \n  var numRowss = $(\"#gridVar001\").getValue();  \n  var numRows = numRowss.length;\n  var puntaje_monetario=0;\n  var v_i =0;\n  var puntaje_total = 0;\n  var fc = false;\n  var ai = false;\n  var dis = false;\n\n  for (var i=1; i <= numRows; i++) {\n\n      v_i = parseFloat($(\"#gridVar001\").getText(i, 5));\n      \n      if(v_i <= x_a){\n        puntaje_monetario = 70 * (1 - ((x_a - v_i) / x_a));\n      }\n      else{\n        puntaje_monetario = 70 * (1 - (2 * ( (Math.abs(x_a - v_i)) / x_a)));\n      }\n      \n      $(\"#gridVar001\").setText(puntaje_monetario.toFixed(7),i, 6);\n\n      fc = $(\"#gridVar001\").getText(i, 2);\n      ai = $(\"#gridVar001\").getText(i, 3);\n      dis = $(\"#gridVar001\").getText(i, 4);\n\n      puntaje_total = puntaje_monetario;\n      \n      if(fc == \"true\"){\n        puntaje_total += 19;\n      }\n      if(ai == \"true\"){\n        puntaje_total += 10;\n      }\n      if(dis == \"true\"){\n        puntaje_total += 1;\n      }\n      \n      $(\"#gridVar001\").setText(puntaje_total.toFixed(7),i, 7);\n      $(\"#textVar003\").setText(x_a.toFixed(2));\n      $(\"#textVar003\").css(\"color\", \"green\");\n  }\n  var ganador = resaltar_ganador();\n}\n\nfunction metodo4(){\t\n\n  var r = organizar_descendente();\n  var v_min = calcular_propuesta_baja();\n  var x = obtener_promedio_general();\n  var x_b = (v_min + x) / 2;\n  \n  var numRowss = $(\"#gridVar001\").getValue();  \n  var numRows = numRowss.length;\n  var puntaje_monetario=0;\n  var v_i =0;\n  var puntaje_total = 0;\n  var fc = false;\n  var ai = false;\n  var dis = false;\n\n  for (var i=1; i <= numRows; i++) {\n\n      v_i = parseFloat($(\"#gridVar001\").getText(i, 5));\n      \n      if(v_i <= x_b){\n        puntaje_monetario = 70 * (1 - ((x_b - v_i) / x_b));\n      }\n      else{\n        puntaje_monetario = 70 * (1 - ( (Math.abs(x_b - v_i)) / x_b));\n      }\n      \n      $(\"#gridVar001\").setText(puntaje_monetario.toFixed(7),i, 6);\n\n      fc = $(\"#gridVar001\").getText(i, 2);\n      ai = $(\"#gridVar001\").getText(i, 3);\n      dis = $(\"#gridVar001\").getText(i, 4);\n\n      puntaje_total = puntaje_monetario;\n      \n      if(fc == \"true\"){\n        puntaje_total += 19;\n      }\n      if(ai == \"true\"){\n        puntaje_total += 10;\n      }\n      if(dis == \"true\"){\n        puntaje_total += 1;\n      }\n      \n      $(\"#gridVar001\").setText(puntaje_total.toFixed(7),i, 7);\n      $(\"#textVar003\").setText(x_b.toFixed(2));\n      $(\"#textVar003\").css(\"color\", \"green\");\n  }\n  var ganador = resaltar_ganador();\n}\n\nfunction metodo5(){\t\n\n  var r = organizar_descendente();\n  var v_min = calcular_propuesta_baja();  \n  \n  var numRowss = $(\"#gridVar001\").getValue();  \n  var numRows = numRowss.length;\n  var puntaje_monetario=0;\n  var v_i =0;\n  var puntaje_total = 0;\n  var fc = false;\n  var ai = false;\n  var dis = false;\n\n  for (var i=1; i <= numRows; i++) {\n\n      v_i = parseFloat($(\"#gridVar001\").getText(i, 5));      \n      \n      puntaje_monetario = (70 * v_min)/v_i;\n      \n      \n      $(\"#gridVar001\").setText(puntaje_monetario.toFixed(7),i, 6);\n\n      fc = $(\"#gridVar001\").getText(i, 2);\n      ai = $(\"#gridVar001\").getText(i, 3);\n      dis = $(\"#gridVar001\").getText(i, 4);\n\n      puntaje_total = puntaje_monetario;\n      \n      if(fc == \"true\"){\n        puntaje_total += 19;\n      }\n      if(ai == \"true\"){\n        puntaje_total += 10;\n      }\n      if(dis == \"true\"){\n        puntaje_total += 1;\n      }\n      \n      $(\"#gridVar001\").setText(puntaje_total.toFixed(7),i, 7);\n      $(\"#textVar003\").setText(propuesta_ideal_m5().toFixed(2));\n      $(\"#textVar003\").css(\"color\", \"green\");\n  }\n  var ganador = resaltar_ganador();\n}\n\nfunction obtener_promedio_general(){\n\n  var numRowss = $(\"#gridVar001\").getValue();  \n  var numRows = numRowss.length;\n  var suma=0;\n\n  for (var i=1; i <= numRows; i++) {\n\n      suma += parseFloat($(\"#gridVar001\").getText(i, 5));          \n  }\n\n  return suma/numRows;\n}\n\nfunction calcular_propuesta_baja(){\n\n  var v_mini = parseFloat($(\"#gridVar001\").getText(1, 5));\n\n  var numRowss = $(\"#gridVar001\").getValue();  \n  var numRows = numRowss.length;  \n\n  for (var i=1; i <= numRows; i++) {\n                \n      if(parseFloat($(\"#gridVar001\").getText(i, 5)) < v_mini){\n        v_mini = parseFloat($(\"#gridVar001\").getText(i, 5));\n      }\n  }\n\n  return v_mini;\n}\n\nfunction calcular_propuesta_alta(){\n\n  var v_maxi = parseFloat($(\"#gridVar001\").getText(1, 5));\n\n  var numRowss = $(\"#gridVar001\").getValue();  \n  var numRows = numRowss.length;\n\n  for (var i=1; i <= numRows; i++) {\n\n      if(parseFloat($(\"#gridVar001\").getText(i, 5)) > v_maxi){\n        v_maxi = parseFloat($(\"#gridVar001\").getText(i, 5));\n      }\n  }\n\n  return v_maxi;\n}\n\nfunction obtener_cantidad_m(){\n\n  var numRowss = $(\"#gridVar001\").getValue();  \n  var numRows = numRowss.length\n\n  if ( numRows > 0 && numRows <= 3)\n  {        \n        return 1;\n  }\n  else if(numRows <= 6 && numRows > 3)\n  {\n        return 2;\n  }\n  else if(numRows <= 9 && numRows > 6)\n  {\n        return 3;     \n  }\n  else if(numRows <= 12 && numRows > 9)\n  {\n        return 4;   \n  }\n  else if(numRows <= 15 && numRows > 12)\n  {\n        return 5;     \n  }\n  else if ( numRows > 15 && numRows <= 18)\n  {        \n        return 6;\n  }\n  else if(numRows <= 21 && numRows > 18)\n  {\n        return 7;\n  }\n  else if(numRows <= 24 && numRows > 21)\n  {\n        return 8;    \n  }\n  else if(numRows <= 27 && numRows > 24)\n  {\n        return 9;\n  }\n  else if(numRows <= 30 && numRows > 27)\n  {\n        return 10;\n  }\n  else if(numRows <= 33 && numRows > 30)\n  {\n        return 11;\n  }\n}\n\nfunction obtener_multiplicacion_valores(){\n  var multi = 1;\n\n  var numRowss = $(\"#gridVar001\").getValue();  \n  var numRows = numRowss.length;\n\n  for (var i=1; i <= numRows; i++) {\n\n      \n      multi = multi * parseFloat($(\"#gridVar001\").getText(i, 5));\n      \n  }\n\n  return multi;\n}\n\nfunction resaltar_ganador(){\n\n  var v_maxi = parseFloat($(\"#gridVar001\").getText(1, 7));\n  var indice = 0;\n\n  var numRowss = $(\"#gridVar001\").getValue();  \n  var numRows = numRowss.length;\n\n  for (var i=1; i <= numRows; i++) {\n\n      if(parseFloat($(\"#gridVar001\").getText(i, 7)) > parseFloat(v_maxi)){\n        v_maxi = parseFloat($(\"#gridVar001\").getText(i, 7));\n        indice = i;        \n      }\n  }\n\n  $(\"#gridVar001\").getControl(indice, 1).css(\"border\",\"green solid 4px\");\n  $(\"#gridVar001\").getControl(indice, 2).css(\"border\",\"green solid 4px\");\n  $(\"#gridVar001\").getControl(indice, 3).css(\"border\",\"green solid 4px\");\n  $(\"#gridVar001\").getControl(indice, 4).css(\"border\",\"green solid 4px\");\n  $(\"#gridVar001\").getControl(indice, 5).css(\"border\",\"green solid 4px\");\n  $(\"#gridVar001\").getControl(indice, 6).css(\"border\",\"green solid 4px\");\n  $(\"#gridVar001\").getControl(indice, 7).css(\"border\",\"green solid 4px\");\n\n  return 1;\n}\n\nfunction desresaltar(){\n  \n  var numRowss = $(\"#gridVar001\").getValue();  \n  var numRows = numRowss.length;\n\n  for (var i=1; i <= numRows; i++) {\n\n      $(\"#gridVar001\").getControl(i, 1).css(\"border\",\"gray solid 1px\");\n      $(\"#gridVar001\").getControl(i, 2).css(\"border\",\"gray solid 1px\");\n      $(\"#gridVar001\").getControl(i, 3).css(\"border\",\"gray solid 1px\");\n      $(\"#gridVar001\").getControl(i, 4).css(\"border\",\"gray solid 1px\");\n      $(\"#gridVar001\").getControl(i, 5).css(\"border\",\"gray solid 1px\");\n      $(\"#gridVar001\").getControl(i, 6).css(\"border\",\"gray solid 1px\");\n      $(\"#gridVar001\").getControl(i, 7).css(\"border\",\"gray solid 1px\");\n  }\n\n      \n\n  return 1;\n}\n\nfunction propuesta_ideal_m1(){\n\n  var numRowss = $(\"#gridVar001\").getValue();  \n  var numRows = numRowss.length;  \n  var recibe = 0;\n  var propuesta = 0;\n\n  if ((numRowss.length + 1) % 2 == 0){\n    \n    recibe = Math.trunc(numRows/2) +1;\n    propuesta = parseFloat($(\"#gridVar001\").getText(recibe, 5)) - 0.01;\n  }\n  else{\n    \n    recibe = Math.trunc(numRows/2) + 1;\n    propuesta = parseFloat($(\"#gridVar001\").getText(recibe, 5)) + 0.01;\n  }\n\n  return propuesta;\n}\n\nfunction propuesta_ideal_m5(){\n\n  var r = organizar_descendente();\n  var v_min = calcular_propuesta_baja();  \n  \n  var numRowss = $(\"#gridVar001\").getValue();  \n  var numRows = numRowss.length;\n  var puntaje_monetario=0;\n  var v_i =0;\n  var puntaje_total = 0;\n  var fc = false;\n  var ai = false;\n  var dis = false;\n\n  var pro_baja = parseFloat(calcular_propuesta_baja()) - 0.01;\n\n  return pro_baja;\n}\n\nfunction formato(){\n\n /*var gridId = \"gridVar001\"; //set to ID of grid\n  var fieldId = \"text0000000003\"; //set to ID of text field inside grid\n\n  var numRowss = $(\"#gridVar001\").getValue();  \n  var numRows = numRowss.length;\n\n  //set the thousands separator in all existing rows when the Dynaform loads:  \n  for (var i = 1; i <= numRows; i++) {\n      //$(\"[id='form[\"+gridId+\"][\"+i+\"][\"+fieldId+\"]']\").maskNumber({decimal: ',', thousands: '.'});\n      $(\"#gridVar001\").getControl(i, 5).maskNumber();\n