        <dyn_filename><![CDATA[2299789195d3f43d8d24df5017312232/3574983295d3f44e77633b2006527710]]></dyn_filename>
        <dyn_content><![CDATA[{"name":"prueba dropdown","description":"","items":[{"type":"form","variable":"","var_uid":"","dataType":"","id":"3574983295d3f44e77633b2006527710","name":"prueba dropdown","description":"","mode":"edit","script":{"type":"js","code":"//when form loads, add options stored in the \"servicesList\" hidden field \n//to the \"selectService\" dropdown. These options were stored from the previous time \n//the Dynaform was filled out by the user.\nvar aServices = $(\"#servicesList\").getValue() == '' ? [] : JSON.parse( $(\"#servicesList\").getValue() );\n\nfor (index in aServices) {\n  var key = aServices[index][0];\n  var label = aServices[index][1];\n  var selected = ( $('#selectService').getValue() == key );\n  \n  //if the option doesn't already exist in the dropdown then add it:\n  if ($(\"#selectService option[value='\"+key+\"']\").length == 0) {\n    $('#selectService').getControl().append( \n      new Option(label, key, false, selected) \n    );\n  }  \n}\n\n\n//add option to \"selectService\" dropdown box when the \"Add Option\" button is clicked:\n$(\"#addOptionBtn\").find(\"button\").click( function() {\n  var newKey = $(\"#newKey\").getValue();\n  var newLabel = $(\"#newLabel\").getValue();\n  var defaultSelected = false;\n  var nowSelected     = true;\n  \n  if (newKey == '' || newLabel == '') {\n    alert(\"Fill in a New Key and New Label before clicking the 'Add Option' button\");\n    return; //do nothing if empty\n  }\n  \n  //add new option to dropdown box:\n  $('#selectService').getControl().append( new Option(newLabel, newKey, defaultSelected, nowSelected) );\n  \n  $(\"#newKey\").setValue('');\n  $(\"#newLabel\").setValue('');\n})\n\n//delete option from the \"selectService\" dropdown box \n//when the \"Delete selected option\" button is clicked:\n$(\"#deleteSelectedBtn\").find(\"button\").click( function() {\n  var selectedKey = $(\"#selectService\").getValue();\n  if (selectedKey != '') {\n    //$('#selectService').getControl().find().remove();\n    //$('#selectService').setValue('');\n    $('#selectService').val(0);\n  }\n})  \n\n//when the DynaForm is submitted, save the list of added options as a JSON string\n//to the \"servicesList\" field, so that those options can be displayed if the Dynaform \n//is displayed a second time  \ngetFormById( $(\"form\").prop(\"id\") ).setOnSubmit( function() { \n  var aServices = [];\n  var aCurrentOpts = $(\"#selectService option\");\n  \n  for (idx = 0; idx < aCurrentOpts.length; idx++) {\n    var key = aCurrentOpts.eq(idx).prop(\"value\")\n    var label = aCurrentOpts.eq(idx).prop(\"text\")\n    aServices.push( [key, label] );\n  }\n  \n  $(\"#servicesList\").setValue( JSON.stringify(aServices) );\n})"},"language":"en","externalLibs":"","printable":false,"items":[[{"type":"subtitle","id":"subtitle0000000001","label":"New Service Option","colSpan":12}],[{"type":"text","variable":"newOption","var_uid":"8159615685d3f43ef62f2a5064956663","dataType":"string","protectedValue":false,"id":"newKey","name":"newOption","label":"New Key","defaultValue":"","placeholder":"","hint":"","required":false,"requiredFieldErrorMessage":"","textTransform":"none","validate":"","validateMessage":"","maxLength":1000,"formula":"","mode":"parent","operation":"","dbConnection":"workflow","dbConnectionLabel":"PM Database","sql":"","var_name":"newOption","colSpan":5},{"type":"text","variable":"newLabel","var_uid":"3986824375d3f43ef6be0e6004421144","dataType":"string","protectedValue":false,"id":"newLabel","name":"newLabel","label":"New Label","defaultValue":"","placeholder":"","hint":"","required":false,"requiredFieldErrorMessage":"","textTransform":"none","validate":"","validateMessage":"","maxLength":1000,"formula":"","mode":"parent","operation":"","dbConnection":"workflow","dbConnectionLabel":"PM Database","sql":"","var_name":"newLabel","colSpan":5},{"type":"button","id":"addOptionBtn","name":"addOptionBtn","label":"Add Option","colSpan":2}],[{"type":"dropdown","variable":"selectService","var_uid":"1003631665d3f43ef776397041465223","dataType":"string","protectedValue":false,"id":"selectService","name":"selectService","label":"Select Service","defaultValue":"","placeholder":"","hint":"","required":false,"requiredFieldErrorMessage":"","mode":"parent","datasource":"database","dbConnection":"workflow","dbConnectionLabel":"PM Database","sql":"","dataVariable":"","options":[{"value":"catering","label":"Catering"},{"value":"accounting","label":"Accounting"},{"value":"lawn_service","label":"Lawn Service"}],"var_name":"selectService","colSpan":10},{"type":"button","id":"deleteSelectedBtn","name":"deleteSelectedBtn","label":"Delete selected Option","colSpan":2}],[{"type":"submit","id":"submit0000000001","name":"submit0000000001","label":"Submit","colSpan":12}],[{"type":"hidden","variable":"servicesList","var_uid":"8689386345d3f43ef8061a1087853783","dataType":"string","protectedValue":false,"id":"servicesList","name":"servicesList","defaultValue":"","dbConnection":"workflow","dbConnectionLabel":"PM Database","sql":"","var_name":"servicesList","colSpan":12}]],"variables":[{"var_uid":"8159615685d3f43ef62f2a5064956663","prj_uid":"2299789195d3f43d8d24df5017312232","var_name":"newOption","var_field_type":"string","var_field_size":10,"var_label":"string","var_dbconnection":"workflow","var_dbconnection_label":"PM Database","var_sql":"","var_null":0,"var_default":"","var_accepted_values":[],"inp_doc_uid":"","var_uid_old":"7916460745b286191dcfe91093835702","var_name_old":"newOption","prj_uid_old":"9763320645b246f9bd50c29070855634","create":false},{"var_uid":"3986824375d3f43ef6be0e6004421144","prj_uid":"2299789195d3f43d8d24df5017312232","var_name":"newLabel","var_field_type":"string","var_field_size":10,"var_label":"string","var_dbconnection":"workflow","var_dbconnection_label":"PM Database","var_sql":"","var_null":0,"var_default":"","var_accepted_values":[],"inp_doc_uid":"","var_uid_old":"1064987095b286579ab2f35038508658","var_name_old":"newLabel","prj